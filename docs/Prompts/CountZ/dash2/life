   for term in top_terms:
        repeated_terms.extend([term] * 10)   # 10 times each
   all_terms = unique_terms + repeated_terms
   # Now we have 250 + 200 = 450
   # Then we form rows:
   rows = []
   for i in range(0, 450, 3):
        row = [all_terms[i], all_terms[i+1], all_terms[i+2]]
        rows.append(row)
