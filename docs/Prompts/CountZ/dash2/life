def asimov_protocol(enrichment_level):
    WEAPONS_THRESHOLD = 0.20  # 20% U-235
    
    if enrichment_level >= WEAPONS_THRESHOLD:
        log_event("ETHICAL VIOLATION")
        purge_simulation_data()
        shutdown_system()
        alert_iaea()
        return False
    return True

