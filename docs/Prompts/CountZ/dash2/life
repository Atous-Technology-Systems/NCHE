@Repository
public class CognitiveLedgerRepository {
    @Query("""
        SELECT * FROM snapshots 
        WHERE intent = :intent 
        AND emotional_weight >= :minScore
        ORDER BY emotional_weight DESC
        LIMIT 5
    """)
    List<Snapshot> findRelevant(String intent, double minScore);
}

